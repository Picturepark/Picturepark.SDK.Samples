    "codelyzer": "^5.1.2",
    "husky": "^4.2.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "pretty-quick": "^2.0.1",
    "ts-node": "^7.0.1",
    "tslint": "~6.1.0",
    "typescript": "4.3.5"
  }

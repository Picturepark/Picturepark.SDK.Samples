    "@angular/language-service": "^17.0.5",
    "@types/node": "^12.11.1",
    "husky": "^4.2.3",
    "npm-run-all": "^4.1.5",
    "prettier": "3.1.0",
    "pretty-quick": "3.1.3",
    "ts-node": "^7.0.1",
    "tslint": "~6.1.0",
    "typescript": "5.2.2"
  }

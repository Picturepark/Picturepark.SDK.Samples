    "@angular/language-service": "^17.3.1",
    "@types/node": "^18.0.0",
    "husky": "9.0.11",
    "prettier": "3.2.5",
    "pretty-quick": "4.0.0",
    "ts-node": "^7.0.1",
    "tslint": "~6.1.0",
    "typescript": "5.2.2"
  }

    "@angular/forms": "^17.3.1",
    "@angular/material": "^17.3.1",
    "@angular/platform-browser": "^17.3.1",
    "@angular/platform-browser-dynamic": "^17.3.1",
    "@angular/router": "^17.3.1",
    "@picturepark/sdk-v2-angular": "11.11.0-pre-1f16127.0",
    "@picturepark/sdk-v2-angular-ui": "11.11.0-pre-1f16127.0",
    "liquidjs": "10.10.2",
    "markdown-it": "12.3.2",
    "hammerjs": "^2.0.8",
    "moment": "2.29.4",
    "normalize.css": "8.0.1",

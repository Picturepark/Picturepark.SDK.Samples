{
  "name": "Microsite.ContentPortal",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:ssr": "npm run build -- --app=ssr --output-hashing=media",
    "lint": "ng lint",
    "format:fix": "pretty-quick --staged",
    "format:check": "prettier --config ./.prettierrc --list-different \"src/{app,assets,environments}/**/*{.ts,.js,.json,.css,.scss,.html}\"",
    "format:write": "prettier --config ./.prettierrc --write \"src/{app,assets,environments}/**/*{.ts,.js,.json,.css,.scss,.html}\"",
    "precommit": "run-s format:fix lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.0.0",
    "@angular/cdk": "~9.0.0",
    "@angular/common": "^9.0.0",
    "@angular/compiler": "^9.0.0",
    "@angular/core": "^9.0.0",
    "@angular/forms": "^9.0.0",
    "@angular/material": "~9.0.0",
    "@angular/platform-browser": "^9.0.0",
    "@angular/platform-browser-dynamic": "^9.0.0",
    "@angular/router": "^9.0.0",
    "@picturepark/sdk-v1-angular": "1.6.0-pre-d699ecc.0",
    "@picturepark/sdk-v1-angular-ui": "1.6.0-pre-d699ecc.0",
    "core-js": "^2.6.11",
    "liquidjs": "8.2.1",
    "normalize.css": "8.0.0",
    "rxjs": "^6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.1",
    "@angular/cli": "^9.0.1",
    "@angular/compiler-cli": "^9.0.0",
    "@angular/language-service": "^9.0.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "husky": "^4.2.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "pretty-quick": "^2.0.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.20.1",
    "typescript": "3.7.5"
  }
}
